{
  "name": "Breezy Recruiting",
  "description": "Easily source candidates",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
  },
  "browser_action": {
      "default_popup": "popup.html"
  },
  "content_scripts": [
      {
        "matches": [
          "*://*/*"
        ],
        "js": [
          "content.js"
        ]
      }
  ],
  "permissions": [
      "tabs",
      "storage",
      "<all_urls>"
  ]
}